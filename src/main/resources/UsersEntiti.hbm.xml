<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ua.dbase.entitis.UsersEntiti" table="users" schema="phonebase">
        <id name="idUser">
            <column name="id_user" sql-type="int(10)"/>
        </id>
        <property name="login">
            <column name="login" sql-type="varchar(15)" length="15"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="userName">
            <column name="user_name" sql-type="varchar(30)" length="30"/>
        </property>
    </class>
</hibernate-mapping>